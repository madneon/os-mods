[org/gnome/desktop/background]
primary-color='#425265'
secondary-color='#141f2a'
color-shading-type='vertical'
picture-options='wallpaper'
picture-uri='file:////usr/share/gnome-control-center/pixmaps/noise-texture-light.png'
show-desktop-icons=true

[org/gnome/desktop/peripherals/keyboard]
delay=uint32 200
repeat-interval=uint32 20

[org/gnome/gnome-session]
logout-prompt=false

[org/gnome/settings-daemon/peripherals/mouse]
double-click=200

[org/gnome/desktop/peripherals/mouse]
accel-profile='flat'
speed=0.25

[org/gnome/desktop/peripherals/touchpad]
natural-scroll=false
speed=0.25
tap-to-click=true
two-finger-scrolling-enabled=true
click-method='areas'

[org/gnome/desktop/privacy]
remove-old-temp-files=true
old-files-age=uint32 3
recent-files-max-age=7

[org/gnome/desktop/sound]
allow-volume-above-100-percent=true

[org/gnome/settings-daemon/plugins/power]
ambient-enabled=false
idle-dim=true
sleep-inactive-battery-timeout=900
sleep-inactive-ac-type='suspend'
sleep-inactive-ac-timeout=3600
power-button-action='interactive'

[org/gnome/settings-daemon/plugins/color]
night-light-enabled=true
night-light-schedule-automatic=true

[org/gnome/desktop/media-handling]
autorun-never=true

[org/gnome/desktop/interface]
gtk-theme='Adwaita-dark'
icon-theme='Adwaita'
menubar-accel=''
cursor-blink-time=400
clock-format='24h'
clock-show-date=true

[org/gnome/shell/extensions/net/gfxmonk/impatience]
speed-factor=0.5

[org/gnome/shell]
enabled-extensions=['impatience@gfxmonk.net', 'alternate-tab@gnome-shell-extensions.gcampax.github.com', 'GPaste@gnome-shell-extensions.gnome.org', 'launch-new-instance@gnome-shell-extensions.gcampax.github.com', 'native-window-placement@gnome-shell-extensions.gcampax.github.com', 'TopIcons@phocean.net', 'windowsNavigator@gnome-shell-extensions.gcampax.github.com', 'hidetopbar@mathieu.bidon.ca']
favorite-apps=['firefox-esr.desktop', 'thunderbird.desktop', 'org.gnome.Nautilus.desktop', 'org.gnome.Terminal.desktop', 'libreoffice-writer.desktop', 'libreoffice-calc.desktop']

[org/gnome/settings-daemon/plugins/xsettings]
hinting='slight'
antialiasing='grayscale'

[org/gnome/desktop/wm/preferences]
resize-with-right-button=true
auto-raise=true
mouse-button-modifier='<Alt>'
action-middle-click-titlebar='minimize'

[org/gnome/mutter]
dynamic-workspaces=false

[org/gnome/deja-dup]
backend='local'
[org/gnome/deja-dup/local]
folder='.backups/deja-dup'

[org/gtk/settings/file-chooser]
sort-directories-first=true
expand-folders=true
startup-mode='cwd'

[org/gnome/shell/extensions/hidetopbar]
mouse-sensitive=true
enable-active-window=true
hot-corner=true
